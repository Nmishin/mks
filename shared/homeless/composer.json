{
    "name": "codepilots/homeless",
    "license": "proprietary",
    "type": "project",
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.2",
        "symfony/http-kernel": "^6.0",
        "symfony/config": "^6.0",
        "symfony/asset": "^6.0",
        "symfony/console": "^6.0",
        "symfony/runtime": "^6.0",
        "symfony/cache": "^6.0",
        "symfony/framework-bundle": "^6.0",
        "symfony/dependency-injection": "^6.0",
        "symfony/templating": "^6.0",
        "symfony/twig-bundle": "^6.0",
        "symfony/monolog-bundle": "^3.0",
        "doctrine/orm": "^2.5",
        "doctrine/doctrine-bundle": "^2.0",
        "doctrine/migrations": "^3.0",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "sensio/framework-extra-bundle": "^6.0",
        "incenteev/composer-parameter-handler": "^2.1",
        "sonata-project/doctrine-orm-admin-bundle": "^4.0",
        "sonata-project/datagrid-bundle": "^3.0",
        "sonata-project/user-bundle": "^5.0",
        "sonata-project/media-bundle": "^4.0",
        "sonata-project/doctrine-extensions": "^1.0",
        "sonata-project/intl-bundle": "^3.0",
        "sonata-project/formatter-bundle": "^5.0",
        "sonata-project/translation-bundle": "^3.0",
        "friendsofsymfony/rest-bundle": "^3.0",
        "vich/uploader-bundle": "^1.4",
        "liip/imagine-bundle": "^2.0",
        "mopa/bootstrap-bundle": "^3.1",
        "twbs/bootstrap-sass": "^3.3",
        "knplabs/knp-snappy-bundle": "^1.4",
        "knplabs/knp-paginator-bundle": "^6.0",
        "h4cc/wkhtmltopdf-amd64": "^0.12.3",
        "laminas/laminas-zendframework-bridge": "^1.5",
        "symfony/webpack-encore-bundle": "^1.8",
        "phpoffice/phpspreadsheet": "^1.23",
        "symfony/flex": "^1.19",
        "symfony/apache-pack": "^1.0",
        "sonata-project/exporter": "^2.0",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^2.12|^3.0",
        "mnvx/lowrapper": "^1.0",
        "twig/intl-extra": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "require-dev": {
        "symfony/maker-bundle": "^1.0",
        "symfony/dotenv": "^6.0",
        "symfony/web-profiler-bundle": "^6.0"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "scripts": {
        "post-install-cmd": [
            "Mopa\\Bundle\\BootstrapBundle\\Composer\\ScriptHandler::postInstallSymlinkTwitterBootstrapSass"
        ],
        "post-update-cmd": [
            "Mopa\\Bundle\\BootstrapBundle\\Composer\\ScriptHandler::postInstallSymlinkTwitterBootstrapSass"
        ],
        "auto-scripts": {
            "security-checker security:check": "script",
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "ckeditor:install": "symfony-cmd"
        }
    },
    "config": {
        "bin-dir": "bin",
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "symfony": {
            "docker": true,
            "allow-contrib": true
        }
    }
}
